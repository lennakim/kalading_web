<div class="container">

  <p id="notice"><%= notice %></p>

  <table class='table'>
    <tr>
      <td>名称</td>
      <td><%= resource.name %></td>
    </tr>
    <tr>
      <td>System Id</td>
      <td><%= resource.system_id %></td>
    </tr>

    <tr>
      <td>产品</td>
      <td>
        <table class="table">

          <% resource.products.each do |product| %>
            <tr>
              <td><%= link_to product.title, admin_product_path(product) %></td>
            </tr>
          <% end %>
        </table>
      </td>
    </tr>
  </table>


  <%= link_to 'Edit', edit_admin_city_path(resource) %> |
  <%= link_to 'Back', admin_cities_path %>

</div>

<div class="container">
  <table class='table'>
    <thead>
      <tr>
        <th>手机号</th>
        <th>创建时间</th>
      </tr>
    </thead>

    <tbody>
      <% collection.each do |user| %>
        <tr>

          <td><%= link_to user.phone_number, admin_user_path(user) %></td>
          <td><%= user.created_at %></td>

        </tr>
      <% end %>
    </tbody>
  </table>
</div>

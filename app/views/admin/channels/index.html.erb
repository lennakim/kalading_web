<div class="container">
  <table class='table'>
    <thead>
      <tr>
        <th>名称</th>
        <th>KEY</th>
        <th>展示</th>
        <th>编辑</th>
        <th>删除</th>
      </tr>
    </thead>

    <tbody>
      <% collection.each do |channel| %>

        <tr>
          <td><%= channel.name %></td>
          <td><%= channel.key %></td>

          <td><%= link_to 'Show', admin_channel_path(channel) %></td>
          <td><%= link_to 'Edit', edit_admin_channel_path(channel) %></td>
          <td><%= link_to 'Destroy', admin_channel_path(channel), method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to '新建', new_admin_channel_path %>
</div>

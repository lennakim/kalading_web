<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th>关键字</th>
        <th>类型</th>
        <th>内容</th>
        <th>编辑</th>
        <th>删除</th>
      </tr>
    </thead>

    <tbody>
      <% collection.each do |msg| %>
        <tr>
          <td><%= msg.keyword %></td>
          <td><%= msg.msg_type %></td>
          <td><%= msg.content %></td>
          <td><%= link_to "编辑", edit_admin_reply_message_path(msg.id) %></td>
          <td><%= link_to "删除", admin_reply_message_path(msg.id), method: :delete, data:{ confirm: '你特么确定要删除我吗?'} %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to "添加", new_admin_reply_message_path %> |
  <%= link_to "返回", admin_weixin_index_path %>
</div>

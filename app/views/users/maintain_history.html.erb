<div class="user-info special">
  <div class="main">

    <div class="row user-info-main clearfix">

      <%= render "users/menu" %>

      <div class="col-md-10 content maintain-report">
        <div class="report">
          <div class="head">
            <%= link_to "<<返回保养报告", maintain_histories_list_users_path %>
          </div>

          <div role="tabpanel" class='tab-panel'>

            <ul class="nav nav-tabs" role="tablist">
              <li role="presentation" class="active">
                <a href="#summary" aria-controls="home" role="tab" data-toggle="tab">保养总结</a>
              </li>
              <li role="presentation">
                <a href="#lights" aria-controls="profile" role="tab" data-toggle="tab">外观/灯光</a>
              </li>
              <li role="presentation">
                <a href="#tires" aria-controls="messages" role="tab" data-toggle="tab">轮胎/刹车</a>
              </li>
              <li role="presentation">
                <a href="#oil" aria-controls="settings" role="tab" data-toggle="tab">油液/滤芯/电瓶</a>
              </li>
              <li role="presentation">
                <a href="#others" aria-controls="settings" role="tab" data-toggle="tab">其他</a>
              </li>
            </ul>

            <div class="tab-content">
              <div role="tabpanel" class="tab-pane fade in active" id="summary">
                <%= render "summary", report: @maintain_history %>
              </div>

              <div role="tabpanel" class="tab-pane fade" id="lights">
                <%= render "appearance", report: @maintain_history %>
              </div>
              <div role="tabpanel" class="tab-pane fade" id="tires">
                <%= render "tire", report: @maintain_history %>
              </div>
              <div role="tabpanel" class="tab-pane fade" id="oil">
                <%= render "elements", report: @maintain_history %>
              </div>

              <div role="tabpanel" class="tab-pane fade" id="others">
                <%= render "others", report: @maintain_history %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
